<div class="row justify-content-end" style="margin: 20px 0;">
      <% if user_signed_in? %>
      <div class="search-form">
        <%= form_with url:search_path, method: :get, local:true do |f| %>
        <%= f.text_field :content %>
        <%= f.select :model, options_for_select({ "ユーザーネーム" => "user", "ダイアリー" => "diary" }) %>
        <%= f.submit '検索' %>
        <% end %>
      </div>
      <% end %>
    </div>